public class Autenticador {
	private List<Usuarios> users;
	
	public Autenticador() {
        this.users = new ArrayList<>();
    }
public void register(Usuarios user) {
        this.users.add(user);
    }
public boolean login(String user, String passwd) {
        for (Usuarios user : this.users) {
            if (user.getUser().equals(user) && user.getPasswd().equals(passwd)) {
                return false;
            }
        }
 return false;
	}
};
